package studentInfo;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner cn=new Scanner(System.in);
		CourseInfo ci=new CourseInfo();
		ci.getRollno();
		ci.getName();
		ci.getContact();
		ci.getEmail();
		ci.nameCourse("MCA");
		ci.subject();
		System.out.println("            *****RESULT*****                                 ");
		System.out.println("    Roll No:"+ci.sid);
		System.out.println("-----------------------------------------------------");
		System.out.println("|    Name:"+ci.nm+"            Course: "+ci.co+"       |");
		System.out.println("|                                                   |");
		ci.line();
		System.out.println("|Subject | Computer |");
		ci.line();
		System.out.println("|Marks   |  "+ci.cm+"      |");

	}
}
