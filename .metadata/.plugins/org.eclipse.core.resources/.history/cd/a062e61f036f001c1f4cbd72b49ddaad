package studentInfo;

public class Result extends CourseInfo {
	int total;
	int per;
	static int pr;
	void subTotal(){
		total=cm+ds+dts+jl+cl+maths;
		System.out.println();
		System.out.println("  Total ="+total+"/600");
		System.out.println();
	}
	void percentage(){
		percentage();
		per=((total*100)/600);
		System.out.println("  Percentage = "+per+" %");
		
	}
	void status(){
		//super(per);
		switch(per)
		{
		 case 1: if(per<75)
			System.out.print("Pass with Distinstion");
		    break;
		}
	}
	

}
