package PatternProgram;

public class Pattern12 {

	public static void main(String[] args) {
		for(int i=1;i<=4;i++)
		{
			for(int k=2;k<=i;k++)
			{
				System.out.print(" ");
			}
			for(int j=1;j<=1;j++)
			{
				System.out.print("*");
			}
			for(int l=9;l>=(i*2);l--)
			{
				System.out.print(" ");
			}
			
			for(int n=1;n<=1;n++)
			{
				System.out.print("*");
			}
			System.out.println();
			
		}
		for(int i =1;i<=1;i++)
		{
			for(int k=4;k>=i;k--)
			{
				System.out.print(" ");
			}
			System.out.print("*");
		}

	}

}
